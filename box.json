{
    "main": "bin/movepress",
    "output": "build/movepress.phar",
    "chmod": "0755",
    "directories": [
        "src",
        "vendor/symfony/console/Resources"
    ],
    "files": [
        "composer.json"
    ],
    "finder": [
        {
            "name": ["*.php", "*.txt"],
            "exclude": [
                "tests",
                "Tests"
            ],
            "notPath": [
                "#humbug/php-scoper#"
            ],
            "in": "vendor"
        }
    ],
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php"
    ],
    "stub": true
}
